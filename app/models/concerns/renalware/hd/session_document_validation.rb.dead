# frozen_string_literal: true

module Renalware
  module HD
    module SessionDocumentValidation
      def self.extended base
        base.singleton_class.instance_eval do
          include ActiveModel::Validations
          validates_associated(
            :info,
            :observations_before,
            :observations_after,
            :dialysis,
            :hdf,
            :complications
          )
        end
      end
    end
  end
end
